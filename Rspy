local function f(o,p,t,s,dr)a:Create(o,TweenInfo.new(t or.3,s or Enum.EasingStyle.Quart,dr or Enum.EasingDirection.Out),p):Play()end
local function g(cl,p)local i=Instance.new(cl)for k,v in pairs(p)do if k~="Parent"then i[k]=v end end if p.Parent then i.Parent=p.Parent end return i end
local function h(p,r)return g("UICorner",{CornerRadius=UDim.new(0,r or 12),Parent=p})end
local function ui(p,col,t)return g("UIStroke",{Color=col or Color3.fromRGB(80,120,200),Thickness=t or 1,Transparency=.5,Parent=p})end
local function glow(p,col,s)return g("ImageLabel",{Size=UDim2.new(1,s or 30,1,s or 30),Position=UDim2.new(.5,-(s or 30)/2,.5,-(s or 30)/2),BackgroundTransparency=1,Image="rbxassetid://5028857084",ImageColor3=col or Color3.fromRGB(100,150,255),ImageTransparency=.8,ZIndex=p.ZIndex-1,Parent=p})end

function j:Notify(title,text,dur)
local sg=d:FindFirstChild("MirageNotify")or g("ScreenGui",{Name="MirageNotify",ResetOnSpawn=false,Parent=d})
local n=g("Frame",{Size=UDim2.new(0,300,0,80),Position=UDim2.new(1,50,1,-100),BackgroundColor3=Color3.fromRGB(15,18,25),Parent=sg})

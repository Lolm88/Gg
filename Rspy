local ic=g("ImageLabel",{Size=UDim2.new(0,24,0,24),Position=UDim2.new(0,15,0,15),BackgroundTransparency=1,Image=e.Bell,ImageColor3=Color3.fromRGB(100,150,255),Parent=n})
g("TextLabel",{Size=UDim2.new(1,-60,0,22),Position=UDim2.new(0,50,0,12),BackgroundTransparency=1,Text=title or"Notice",TextColor3=Color3.fromRGB(240,245,255),TextSize=14,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=n})
g("TextLabel",{Size=UDim2.new(1,-60,0,18),Position=UDim2.new(0,50,0,38),BackgroundTransparency=1,Text=text or"",TextColor3=Color3.fromRGB(140,150,180),TextSize=12,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,TextTruncate=Enum.TextTruncate.AtEnd,Parent=n})
local pb=g("Frame",{Size=UDim2.new(1,0,0,3),Position=UDim2.new(0,0,1,-3),BackgroundColor3=Color3.fromRGB(100,150,255),Parent=n})
h(pb,2)
f(n,{Position=UDim2.new(1,-320,1,-100)},.4)
spawn(function()
local d=dur or 3
for x=100,0,-1 do pb.Size=UDim2.new(x/100,0,0,3)wait(d/100)end
f(n,{Position=UDim2.new(1,50,1,-100)},.3)wait(.3)n:Destroy()
end)
return n
end

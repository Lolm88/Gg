local function updateNP()for idx,n in ipairs(nq)do if n and n.Parent then f(n,{Position=UDim2.new(1,-320,1,-20-(idx*90))},.3,Enum.EasingStyle.Back)end end end
function j:Notify(title,text,dur,icon)
local sg=d:FindFirstChild("MirageNotify")or g("ScreenGui",{Name="MirageNotify",ResetOnSpawn=false,Parent=d})
local n=g("Frame",{Size=UDim2.new(0,300,0,80),Position=UDim2.new(1,320,1,-110),BackgroundColor3=Color3.fromRGB(15,18,25),Parent=sg})
h(n,14)local nst=ui(n,Color3.fromRGB(100,150,255),1.5)
local ngl=glow(n,Color3.fromRGB(80,120,200),40)ngl.ImageTransparency=1
table.insert(nq,n)updateNP()
local ic=g("ImageLabel",{Size=UDim2.new(0,28,0,28),Position=UDim2.new(0,14,0,14),BackgroundTransparency=1,Image=e[icon]or e.Bell,ImageColor3=Color3.fromRGB(100,150,255),Parent=n})
g("TextLabel",{Size=UDim2.new(1,-60,0,22),Position=UDim2.new(0,52,0,12),BackgroundTransparency=1,Text=title or"Notice",TextColor3=Color3.fromRGB(240,245,255),TextSize=14,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=n})
g("TextLabel",{Size=UDim2.new(1,-60,0,18),Position=UDim2.new(0,52,0,38),BackgroundTransparency=1,Text=text or"",TextColor3=Color3.fromRGB(140,150,180),TextSize=12,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,TextTruncate=Enum.TextTruncate.AtEnd,Parent=n})
local pb=g("Frame",{Size=UDim2.new(1,-20,0,4),Position=UDim2.new(0,10,1,-12),BackgroundColor3=Color3.fromRGB(35,40,50),Parent=n})
h(pb,2)
local pf=g("Frame",{Size=UDim2.new(1,0,1,0),BackgroundColor3=Color3.fromRGB(100,150,255),Parent=pb})
h(pf,2)
local pfg=Instance.new("UIGradient",pf)pfg.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(100,150,255)),ColorSequenceKeypoint.new(1,Color3.fromRGB(150,100,255))}
f(ngl,{ImageTransparency=.7},.4)f(ic,{Rotation=360},.5,Enum.EasingStyle.Back)
spawn(function()
local dt=dur or 3
for x=100,0,-1 do pf.Size=UDim2.new(x/100,0,1,0)pfg.Rotation=(100-x)*3.6 wait(dt/100)end
f(n,{Position=UDim2.new(1,320,n.Position.Y.Scale,n.Position.Y.Offset),BackgroundTransparency=1},.4,Enum.EasingStyle.Back,Enum.EasingDirection.In)
f(nst,{Transparency=1},.3)f(ngl,{ImageTransparency=1},.3)
wait(.4)for idx,v in ipairs(nq)do if v==n then table.remove(nq,idx)break end end n:Destroy()updateNP()
end)
return n
end

h(tind,2)
local tcont=g("Frame",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,Visible=false,Parent=cont})
g("UIListLayout",{Padding=UDim.new(0,8),SortOrder=Enum.SortOrder.LayoutOrder,Parent=tcont})
tabdata[name]={btn=tbtn,cont=tcont,stroke=tst,label=tname,icon=tic,ind=tind}
local function selectTab()tbtn.BackgroundColor3=Color3.fromRGB(80,120,200)tst.Color=Color3.fromRGB(100,150,255)tst.Transparency=.3 tname.TextColor3=Color3.fromRGB(255,255,255)tic.ImageColor3=Color3.fromRGB(255,255,255)f(tind,{Size=UDim2.new(0,3,0.7,0)},.3,Enum.EasingStyle.Back)f(tic,{Rotation=360},.4)task.delay(.4,function()tic.Rotation=0 end)end
local function deselectTab()f(tbtn,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)f(tst,{Color=Color3.fromRGB(40,50,70),Transparency=.5},.2)f(tname,{TextColor3=Color3.fromRGB(180,190,220)},.2)f(tic,{ImageColor3=Color3.fromRGB(140,160,200)},.2)f(tind,{Size=UDim2.new(0,3,0,0)},.2)end
if not curtab then curtab=name tcont.Visible=true selectTab()end
tbtn.MouseButton1Click:Connect(function()
if curtab~=name then
if curtab and tabdata[curtab]then tabdata[curtab].cont.Visible=false for k,v in pairs(tabdata)do if k==curtab then f(v.btn,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)f(v.stroke,{Color=Color3.fromRGB(40,50,70),Transparency=.5},.2)f(v.label,{TextColor3=Color3.fromRGB(180,190,220)},.2)f(v.icon,{ImageColor3=Color3.fromRGB(140,160,200)},.2)f(v.ind,{Size=UDim2.new(0,3,0,0)},.2)end end end
curtab=name tcont.Visible=true selectTab()cont.CanvasPosition=Vector2.new(0,0)stb.Text=""
for _,el in pairs(allelements)do el.frame.Visible=(el.tab==name)end
end
end)
tbtn.MouseEnter:Connect(function()if curtab~=name then f(tbtn,{BackgroundColor3=Color3.fromRGB(30,36,48)},.2)f(tst,{Color=Color3.fromRGB(60,80,120)},.2)f(tind,{Size=UDim2.new(0,3,0.3,0)},.2)end end)

end
function t:Toggle(text,ic,default,callback)
local on=default or false
local tg=g("Frame",{Size=UDim2.new(1,-10,0,44),BackgroundColor3=Color3.fromRGB(20,24,32),Parent=tcont})
h(tg,10)local tgst=ui(tg,on and Color3.fromRGB(100,150,255)or Color3.fromRGB(50,60,80))tgst.Transparency=on and.3 or.5
local tggl=glow(tg,Color3.fromRGB(80,120,200),25)tggl.ImageTransparency=on and.8 or 1
g("ImageLabel",{Size=UDim2.new(0,20,0,20),Position=UDim2.new(0,14,0.5,-10),BackgroundTransparency=1,Image=e[ic]or e.Check,ImageColor3=Color3.fromRGB(100,150,255),Parent=tg})
g("TextLabel",{Size=UDim2.new(1,-110,1,0),Position=UDim2.new(0,42,0,0),BackgroundTransparency=1,Text=text or"Toggle",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=tg})
local sw=g("Frame",{Size=UDim2.new(0,52,0,28),Position=UDim2.new(1,-66,0.5,-14),BackgroundColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(35,40,50),Parent=tg})
h(sw,14)
local kn=g("Frame",{Size=UDim2.new(0,22,0,22),Position=on and UDim2.new(1,-25,0.5,-11)or UDim2.new(0,3,0.5,-11),BackgroundColor3=Color3.fromRGB(255,255,255),Parent=sw})
h(kn,11)local kngl=glow(kn,Color3.fromRGB(255,255,255),15)kngl.ImageTransparency=on and.6 or 1
local chk=g("ImageLabel",{Size=UDim2.new(0,12,0,12),Position=UDim2.new(0.5,-6,0.5,-6),BackgroundTransparency=1,Image=e.Check,ImageColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(60,70,90),ImageTransparency=on and 0 or 1,Parent=kn})
local btn=g("TextButton",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,Text="",Parent=tg})
table.insert(allelements,{frame=tg,name=text or"Toggle",tab=name})
btn.MouseButton1Click:Connect(function()
on=not on
f(sw,{BackgroundColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(35,40,50)},.25,Enum.EasingStyle.Quart)
f(kn,{Position=on and UDim2.new(1,-25,0.5,-11)or UDim2.new(0,3,0.5,-11)},.25,Enum.EasingStyle.Back)
f(kngl,{ImageTransparency=on and.6 or 1},.25)
f(chk,{ImageColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(60,70,90),ImageTransparency=on and 0 or 1},.2)
f(tgst,{Color=on and Color3.fromRGB(100,150,255)or Color3.fromRGB(50,60,80),Transparency=on and.3 or.5},.2)
f(tggl,{ImageTransparency=on and.8 or 1},.25)
if callback then callback(on)end
end)
btn.MouseEnter:Connect(function()f(tg,{BackgroundColor3=Color3.fromRGB(25,30,40)},.2)end)
btn.MouseLeave:Connect(function()f(tg,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)end)
return{Set=function(v)on=v f(sw,{BackgroundColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(35,40,50)},.2)f(kn,{Position=on and UDim2.new(1,-25,0.5,-11)or UDim2.new(0,3,0.5,-11)},.2)f(chk,{ImageTransparency=on and 0 or 1},.2)end,Get=function()return on end}
end
function t:Slider(text,ic,min,max,default,callback)
local val=default or min
local sl=g("Frame",{Size=UDim2.new(1,-10,0,90),BackgroundColor3=Color3.fromRGB(15,18,25),Parent=tcont})
h(sl,14)local slst=ui(sl,Color3.fromRGB(40,50,70))
local ibox=g("Frame",{Size=UDim2.new(0,50,0,50),Position=UDim2.new(0,12,0,10),BackgroundColor3=Color3.fromRGB(80,120,200),Parent=sl})
h(ibox,13)
local ibg=Instance.new("UIGradient",ibox)ibg.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(100,140,220)),ColorSequenceKeypoint.new(1,Color3.fromRGB(140,100,220))}ibg.Rotation=45
g("ImageLabel",{Size=UDim2.new(0,28,0,28),Position=UDim2.new(0.5,-14,0.5,-14),BackgroundTransparency=1,Image=e[ic]or e.Slider,ImageColor3=Color3.fromRGB(255,255,255),Parent=ibox})
g("TextLabel",{Size=UDim2.new(1,-80,0,22),Position=UDim2.new(0,72,0,10),BackgroundTransparency=1,Text=text or"Slider",TextColor3=Color3.fromRGB(240,245,255),TextSize=15,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=sl})
g("TextLabel",{Size=UDim2.new(1,-80,0,16),Position=UDim2.new(0,72,0,36),BackgroundTransparency=1,Text="Adjust value",TextColor3=Color3.fromRGB(110,120,150),TextSize=12,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,Parent=sl})
local bar=g("Frame",{Size=UDim2.new(1,-90,0,24),Position=UDim2.new(0,72,1,-32),BackgroundColor3=Color3.fromRGB(25,30,40),Parent=sl})
h(bar,12)local bst=ui(bar,Color3.fromRGB(50,60,80))
local fill=g("Frame",{Size=UDim2.new((val-min)/(max-min),0,1,0),BackgroundColor3=Color3.fromRGB(100,150,255),Parent=bar})
h(fill,12)
local fg=Instance.new("UIGradient",fill)fg.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(100,150,255)),ColorSequenceKeypoint.new(1,Color3.fromRGB(150,100,255))}
local fgl=glow(fill,Color3.fromRGB(100,150,255),20)fgl.ImageTransparency=.7
local vbx=g("Frame",{Size=UDim2.new(0,60,0,24),Position=UDim2.new(1,-72,1,-32),BackgroundColor3=Color3.fromRGB(25,30,40),Parent=sl})
h(vbx,8)ui(vbx,Color3.fromRGB(50,60,80))
local vlbl=g("TextLabel",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,Text=tostring(val),TextColor3=Color3.fromRGB(200,220,255),TextSize=12,Font=Enum.Font.GothamBold,Parent=vbx})
table.insert(allelements,{frame=sl,name=text or"Slider",tab=name})
local dragging=false
local function upd(x)
local bx=bar.AbsolutePosition.X
local bw=bar.AbsoluteSize.X
local pct=math.clamp((x-bx)/bw,0,1)
val=math.floor(min+(max-min)*pct)
vlbl.Text=tostring(val)
f(fill,{Size=UDim2.new(pct,0,1,0)},.1)
if callback then callback(val)end
end
local btn=g("TextButton",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,Text="",Parent=bar})
btn.InputBegan:Connect(function(inp)if inp.UserInputType==Enum.UserInputType.MouseButton1 then dragging=true upd(inp.Position.X)f(bst,{Color=Color3.fromRGB(100,150,255),Transparency=.3},.2)f(slst,{Color=Color3.fromRGB(80,120,200),Transparency=.3},.2)f(fgl,{ImageTransparency=.5},.2)end end)
btn.InputEnded:Connect(function(inp)if inp.UserInputType==Enum.UserInputType.MouseButton1 then dragging=false f(bst,{Color=Color3.fromRGB(50,60,80),Transparency=.5},.2)f(slst,{Color=Color3.fromRGB(40,50,70),Transparency=.5},.2)f(fgl,{ImageTransparency=.7},.2)end end)
b.InputChanged:Connect(function(inp)if dragging and inp.UserInputType==Enum.UserInputType.MouseMovement then upd(inp.Position.X)end end)
b.InputEnded:Connect(function(inp)if inp.UserInputType==Enum.UserInputType.MouseButton1 and dragging then dragging=false f(bst,{Color=Color3.fromRGB(50,60,80),Transparency=.5},.2)f(slst,{Color=Color3.fromRGB(40,50,70),Transparency=.5},.2)f(fgl,{ImageTransparency=.7},.2)end end)
return{Set=function(v)val=v vlbl.Text=tostring(v)local pct=(v-min)/(max-min)f(fill,{Size=UDim2.new(pct,0,1,0)},.2)end,Get=function()return val end}
end
function t:Dropdown(text,ic,options,default,callback)
local sel=default or options[1]or""
local expanded=false
local dd=g("Frame",{Size=UDim2.new(1,-10,0,44),BackgroundColor3=Color3.fromRGB(20,24,32),ClipsDescendants=true,Parent=tcont})
h(dd,10)local ddst=ui(dd,Color3.fromRGB(50,60,80))
g("ImageLabel",{Size=UDim2.new(0,20,0,20),Position=UDim2.new(0,14,0,12),BackgroundTransparency=1,Image=e[ic]or e.Dropdown,ImageColor3=Color3.fromRGB(100,150,255),Parent=dd})
g("TextLabel",{Size=UDim2.new(1,-150,0,44),Position=UDim2.new(0,42,0,0),BackgroundTransparency=1,Text=text or"Dropdown",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=dd})
local selv=g("TextLabel",{Size=UDim2.new(0,80,0,26),Position=UDim2.new(1,-115,0,9),BackgroundColor3=Color3.fromRGB(30,35,45),Text=sel,TextColor3=Color3.fromRGB(150,170,220),TextSize=11,Font=Enum.Font.Gotham,Parent=dd})
h(selv,8)
local arr=g("ImageLabel",{Size=UDim2.new(0,14,0,14),Position=UDim2.new(1,-28,0,15),BackgroundTransparency=1,Image=e.Arrow,ImageColor3=Color3.fromRGB(100,150,255),Rotation=90,Parent=dd})
local optc=g("Frame",{Size=UDim2.new(1,-10,0,0),Position=UDim2.new(0,5,0,48),BackgroundTransparency=1,Parent=dd})
g("UIListLayout",{Padding=UDim.new(0,4),SortOrder=Enum.SortOrder.LayoutOrder,Parent=optc})
table.insert(allelements,{frame=dd,name=text or"Dropdown",tab=name})
for _,opt in ipairs(options)do
local ob=g("TextButton",{Size=UDim2.new(1,0,0,32),BackgroundColor3=Color3.fromRGB(25,30,40),Text="",AutoButtonColor=false,Parent=optc})
h(ob,8)
local oic=g("ImageLabel",{Size=UDim2.new(0,14,0,14),Position=UDim2.new(0,10,0.5,-7),BackgroundTransparency=1,Image=e.Check,ImageColor3=Color3.fromRGB(100,150,255),ImageTransparency=sel==opt and 0 or 1,Parent=ob})
g("TextLabel",{Size=UDim2.new(1,-40,1,0),Position=UDim2.new(0,30,0,0),BackgroundTransparency=1,Text=opt,TextColor3=Color3.fromRGB(180,190,220),TextSize=12,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,Parent=ob})
ob.MouseEnter:Connect(function()f(ob,{BackgroundColor3=Color3.fromRGB(60,90,150)},.15)end)
ob.MouseLeave:Connect(function()f(ob,{BackgroundColor3=Color3.fromRGB(25,30,40)},.15)end)
ob.MouseButton1Click:Connect(function()
for _,c in pairs(optc:GetChildren())do if c:IsA("TextButton")then local ci=c:FindFirstChild("ImageLabel")if ci then f(ci,{ImageTransparency=1},.15)end end end
f(oic,{ImageTransparency=0},.15)
sel=opt selv.Text=opt expanded=false f(dd,{Size=UDim2.new(1,-10,0,44)},.25,Enum.EasingStyle.Back)f(arr,{Rotation=90},.2)f(ddst,{Color=Color3.fromRGB(50,60,80)},.2)if callback then callback(sel)end
end)
end
local hbtn=g("TextButton",{Size=UDim2.new(1,0,0,44),BackgroundTransparency=1,Text="",Parent=dd})
hbtn.MouseButton1Click:Connect(function()expanded=not expanded local ht=expanded and(48+#options*36)or 44 f(dd,{Size=UDim2.new(1,-10,0,ht)},.3,Enum.EasingStyle.Back)f(arr,{Rotation=expanded and 270 or 90},.25)f(ddst,{Color=expanded and Color3.fromRGB(100,150,255)or Color3.fromRGB(50,60,80)},.2)end)
return{Set=function(v)sel=v selv.Text=v end,Get=function()return sel end}
end
function t:MultiDropdown(text,ic,options,defaults,callback)
local selected=defaults or{}
local expanded=false
local dd=g("Frame",{Size=UDim2.new(1,-10,0,44),BackgroundColor3=Color3.fromRGB(20,24,32),ClipsDescendants=true,Parent=tcont})
h(dd,10)local ddst=ui(dd,Color3.fromRGB(50,60,80))
g("ImageLabel",{Size=UDim2.new(0,20,0,20),Position=UDim2.new(0,14,0,12),BackgroundTransparency=1,Image=e[ic]or e.List,ImageColor3=Color3.fromRGB(100,150,255),Parent=dd})
g("TextLabel",{Size=UDim2.new(1,-150,0,44),Position=UDim2.new(0,42,0,0),BackgroundTransparency=1,Text=text or"Multi Select",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=dd})
local selv=g("TextLabel",{Size=UDim2.new(0,80,0,26),Position=UDim2.new(1,-115,0,9),BackgroundColor3=Color3.fromRGB(30,35,45),Text=#selected.." selected",TextColor3=Color3.fromRGB(150,170,220),TextSize=10,Font=Enum.Font.Gotham,Parent=dd})
h(selv,8)
local arr=g("ImageLabel",{Size=UDim2.new(0,14,0,14),Position=UDim2.new(1,-28,0,15),BackgroundTransparency=1,Image=e.Arrow,ImageColor3=Color3.fromRGB(100,150,255),Rotation=90,Parent=dd})
local optc=g("Frame",{Size=UDim2.new(1,-10,0,0),Position=UDim2.new(0,5,0,48),BackgroundTransparency=1,Parent=dd})
g("UIListLayout",{Padding=UDim.new(0,4),SortOrder=Enum.SortOrder.LayoutOrder,Parent=optc})
table.insert(allelements,{frame=dd,name=text or"MultiDropdown",tab=name})
local function hasVal(t,v)for _,x in pairs(t)do if x==v then return true end end return false end
local function remVal(t,v)for i,x in pairs(t)do if x==v then table.remove(t,i)return end end end
for _,opt in ipairs(options)do
local isOn=hasVal(selected,opt)
local ob=g("TextButton",{Size=UDim2.new(1,0,0,32),BackgroundColor3=Color3.fromRGB(25,30,40),Text="",AutoButtonColor=false,Parent=optc})
h(ob,8)
local obox=g("Frame",{Size=UDim2.new(0,18,0,18),Position=UDim2.new(0,8,0.5,-9),BackgroundColor3=isOn and Color3.fromRGB(80,120,200)or Color3.fromRGB(40,45,55),Parent=ob})
h(obox,5)
local ochk=g("ImageLabel",{Size=UDim2.new(0,12,0,12),Position=UDim2.new(0.5,-6,0.5,-6),BackgroundTransparency=1,Image=e.Check,ImageColor3=Color3.fromRGB(255,255,255),ImageTransparency=isOn and 0 or 1,Parent=obox})
g("TextLabel",{Size=UDim2.new(1,-40,1,0),Position=UDim2.new(0,32,0,0),BackgroundTransparency=1,Text=opt,TextColor3=Color3.fromRGB(180,190,220),TextSize=12,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,Parent=ob})
ob.MouseEnter:Connect(function()f(ob,{BackgroundColor3=Color3.fromRGB(35,42,55)},.15)end)
ob.MouseLeave:Connect(function()f(ob,{BackgroundColor3=Color3.fromRGB(25,30,40)},.15)end)
ob.MouseButton1Click:Connect(function()
local now=hasVal(selected,opt)
if now then remVal(selected,opt)else table.insert(selected,opt)end
local newOn=not now
f(obox,{BackgroundColor3=newOn and Color3.fromRGB(80,120,200)or Color3.fromRGB(40,45,55)},.2)
f(ochk,{ImageTransparency=newOn and 0 or 1},.2)
selv.Text=#selected.." selected"
if callback then callback(selected)end
end)
end
local hbtn=g("TextButton",{Size=UDim2.new(1,0,0,44),BackgroundTransparency=1,Text="",Parent=dd})
hbtn.MouseButton1Click:Connect(function()expanded=not expanded local ht=expanded and(48+#options*36)or 44 f(dd,{Size=UDim2.new(1,-10,0,ht)},.3,Enum.EasingStyle.Back)f(arr,{Rotation=expanded and 270 or 90},.25)f(ddst,{Color=expanded and Color3.fromRGB(100,150,255)or Color3.fromRGB(50,60,80)},.2)end)
return{Set=function(v)selected=v selv.Text=#selected.." selected"end,Get=function()return selected end}
end
function t:Input(text,ic,placeholder,callback)
local inp=g("Frame",{Size=UDim2.new(1,-10,0,40),BackgroundColor3=Color3.fromRGB(15,15,22),Parent=tcont})
h(inp,12)local ist=ui(inp,Color3.fromRGB(60,80,120))
g("ImageLabel",{Size=UDim2.new(0,18,0,18),Position=UDim2.new(0,10,0.5,-9),BackgroundTransparency=1,Image=e[ic]or e.Search,ImageColor3=Color3.fromRGB(100,150,255),Parent=inp})
local tb=g("TextBox",{Size=UDim2.new(1,-40,1,0),Position=UDim2.new(0,40,0,0),BackgroundColor3=Color3.fromRGB(30,35,45),BackgroundTransparency=1,Text="",PlaceholderText=placeholder or text,TextColor3=Color3.fromRGB(220,220,255),PlaceholderColor3=Color3.fromRGB(100,110,150),TextSize=13,Font=Enum.Font.Gotham,TextXAlignment=Enum.TextXAlignment.Left,ClearTextOnFocus=false,Parent=inp})
table.insert(allelements,{frame=inp,name=text or"Input",tab=name})
tb.Focused:Connect(function()f(ist,{Color=Color3.fromRGB(100,150,255),Transparency=.2,Thickness=1.5},.3)f(inp,{BackgroundColor3=Color3.fromRGB(20,25,35)},.3)end)
tb.FocusLost:Connect(function(enter)f(ist,{Color=Color3.fromRGB(60,80,120),Transparency=.5,Thickness=1},.3)f(inp,{BackgroundColor3=Color3.fromRGB(15,15,22)},.3)if enter and callback then callback(tb.Text)end end)
return{Set=function(v)tb.Text=v end,Get=function()return tb.Text end}
end
function t:Label(text)
local lb=g("TextLabel",{Size=UDim2.new(1,-10,0,28),BackgroundTransparency=1,Text=text or"Label",TextColor3=Color3.fromRGB(100,120,160),TextSize=12,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=tcont})
table.insert(allelements,{frame=lb,name=text or"Label",tab=name})
return lb
end
function t:Separator()
local sp=g("Frame",{Size=UDim2.new(1,-30,0,2),BackgroundColor3=Color3.fromRGB(40,50,70),Parent=tcont})
h(sp,1)
local spg=Instance.new("UIGradient",sp)spg.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(40,50,70)),ColorSequenceKeypoint.new(.5,Color3.fromRGB(100,150,255)),ColorSequenceKeypoint.new(1,Color3.fromRGB(40,50,70))}spg.Transparency=NumberSequence.new{NumberSequenceKeypoint.new(0,1),NumberSequenceKeypoint.new(.5,0),NumberSequenceKeypoint.new(1,1)}
return sp
end
function t:Keybind(text,ic,default,callback)
local key=default or Enum.KeyCode.E
local listening=false
local kb=g("Frame",{Size=UDim2.new(1,-10,0,44),BackgroundColor3=Color3.fromRGB(20,24,32),Parent=tcont})
h(kb,10)local kbst=ui(kb,Color3.fromRGB(50,60,80))
g("ImageLabel",{Size=UDim2.new(0,20,0,20),Position=UDim2.new(0,14,0.5,-10),BackgroundTransparency=1,Image=e[ic]or e.Settings,ImageColor3=Color3.fromRGB(100,150,255),Parent=kb})
g("TextLabel",{Size=UDim2.new(1,-130,1,0),Position=UDim2.new(0,42,0,0),BackgroundTransparency=1,Text=text or"Keybind",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=kb})
local kbtn=g("TextButton",{Size=UDim2.new(0,70,0,28),Position=UDim2.new(1,-84,0.5,-14),BackgroundColor3=Color3.fromRGB(30,35,45),Text=key.Name,TextColor3=Color3.fromRGB(150,170,220),TextSize=11,Font=Enum.Font.GothamBold,Parent=kb})
h(kbtn,8)local kbnst=ui(kbtn,Color3.fromRGB(50,60,80))
table.insert(allelements,{frame=kb,name=text or"Keybind",tab=name})
kbtn.MouseButton1Click:Connect(function()listening=true kbtn.Text="..."f(kbnst,{Color=Color3.fromRGB(100,150,255)},.2)f(kbst,{Color=Color3.fromRGB(100,150,255),Transparency=.3},.2)end)
b.InputBegan:Connect(function(inp,gpe)
if listening and inp.UserInputType==Enum.UserInputType.Keyboard then
key=inp.KeyCode kbtn.Text=key.Name listening=false
f(kbnst,{Color=Color3.fromRGB(50,60,80)},.2)f(kbst,{Color=Color3.fromRGB(50,60,80),Transparency=.5},.2)
if callback then callback(key)end
end
end)
return{Set=function(v)key=v kbtn.Text=v.Name end,Get=function()return key end}
end
function t:ColorPicker(text,ic,default,callback)
local col=default or Color3.fromRGB(100,150,255)
local expanded=false
local cp=g("Frame",{Size=UDim2.new(1,-10,0,44),BackgroundColor3=Color3.fromRGB(20,24,32),ClipsDescendants=true,Parent=tcont})
h(cp,10)local cpst=ui(cp,Color3.fromRGB(50,60,80))
g("ImageLabel",{Size=UDim2.new(0,20,0,20),Position=UDim2.new(0,14,0,12),BackgroundTransparency=1,Image=e[ic]or e.Eye,ImageColor3=Color3.fromRGB(100,150,255),Parent=cp})
g("TextLabel",{Size=UDim2.new(1,-130,0,44),Position=UDim2.new(0,42,0,0),BackgroundTransparency=1,Text=text or"Color",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=cp})
local preview=g("Frame",{Size=UDim2.new(0,50,0,26),Position=UDim2.new(1,-64,0,9),BackgroundColor3=col,Parent=cp})
h(preview,8)ui(preview,Color3.fromRGB(60,80,120))
local colors={Color3.fromRGB(255,0,0),Color3.fromRGB(255,127,0),Color3.fromRGB(255,255,0),Color3.fromRGB(0,255,0),Color3.fromRGB(0,255,255),Color3.fromRGB(0,127,255),Color3.fromRGB(0,0,255),Color3.fromRGB(127,0,255),Color3.fromRGB(255,0,255),Color3.fromRGB(255,255,255),Color3.fromRGB(180,180,180),Color3.fromRGB(100,100,100),Color3.fromRGB(50,50,50),Color3.fromRGB(0,0,0)}
local grid=g("Frame",{Size=UDim2.new(1,-20,0,60),Position=UDim2.new(0,10,0,50),BackgroundTransparency=1,Parent=cp})
g("UIGridLayout",{CellSize=UDim2.new(0,36,0,26),CellPadding=UDim2.new(0,6,0,6),Parent=grid})
table.insert(allelements,{frame=cp,name=text or"ColorPicker",tab=name})
for _,c in ipairs(colors)do
local cb=g("TextButton",{Size=UDim2.new(0,36,0,26),BackgroundColor3=c,Text="",Parent=grid})
h(cb,6)
cb.MouseButton1Click:Connect(function()col=c preview.BackgroundColor3=c if callback then callback(c)end end)
cb.MouseEnter:Connect(function()f(cb,{Size=UDim2.new(0,38,0,28)},.1,Enum.EasingStyle.Back)end)
cb.MouseLeave:Connect(function()f(cb,{Size=UDim2.new(0,36,0,26)},.1)end)
end
local hbtn=g("TextButton",{Size=UDim2.new(1,0,0,44),BackgroundTransparency=1,Text="",Parent=cp})
hbtn.MouseButton1Click:Connect(function()expanded=not expanded f(cp,{Size=UDim2.new(1,-10,0,expanded and 120 or 44)},.3,Enum.EasingStyle.Back)f(cpst,{Color=expanded and Color3.fromRGB(100,150,255)or Color3.fromRGB(50,60,80)},.2)end)
return{Set=function(v)col=v preview.BackgroundColor3=v end,Get=function()return col end}
end
return t
end
function w:Notify(title,text,dur,icon)return j:Notify(title,text,dur,icon)end
function w:Destroy()sg:Destroy()end
return w
end
return j

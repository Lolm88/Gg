function w:Tab(name,icon)
local t={}
local tbtn=g("TextButton",{Size=UDim2.new(1,-12,0,38),Position=UDim2.new(0,6,0,0),BackgroundColor3=Color3.fromRGB(20,24,32),Text="",Parent=tabs})
h(tbtn,10)local tst=i(tbtn,Color3.fromRGB(40,50,70))
g("ImageLabel",{Size=UDim2.new(0,18,0,18),Position=UDim2.new(0,10,0.5,-9),BackgroundTransparency=1,Image=e[icon]or e.Home,ImageColor3=Color3.fromRGB(140,160,200),Parent=tbtn})
local tname=g("TextLabel",{Size=UDim2.new(1,-40,1,0),Position=UDim2.new(0,34,0,0),BackgroundTransparency=1,Text=name or"Tab",TextColor3=Color3.fromRGB(180,190,220),TextSize=12,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=tbtn})
local tcont=g("Frame",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,Visible=false,Parent=cont})
local tcl=g("UIListLayout",{Padding=UDim.new(0,8),SortOrder=Enum.SortOrder.LayoutOrder,Parent=tcont})
tabdata[name]={btn=tbtn,cont=tcont,stroke=tst,label=tname}
if not curtab then curtab=name tcont.Visible=true tbtn.BackgroundColor3=Color3.fromRGB(80,120,200)tst.Color=Color3.fromRGB(100,150,255)tname.TextColor3=Color3.fromRGB(255,255,255)end
tbtn.MouseButton1Click:Connect(function()
if curtab~=name then
if curtab and tabdata[curtab]then
local old=tabdata[curtab]
f(old.btn,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)
f(old.stroke,{Color=Color3.fromRGB(40,50,70)},.2)
f(old.label,{TextColor3=Color3.fromRGB(180,190,220)},.2)
old.cont.Visible=false
end
curtab=name tcont.Visible=true
f(tbtn,{BackgroundColor3=Color3.fromRGB(80,120,200)},.2)
f(tst,{Color=Color3.fromRGB(100,150,255)},.2)
f(tname,{TextColor3=Color3.fromRGB(255,255,255)},.2)
end
end)
tbtn.MouseEnter:Connect(function()if curtab~=name then f(tbtn,{BackgroundColor3=Color3.fromRGB(30,36,48)},.2)end end)
tbtn.MouseLeave:Connect(function()if curtab~=name then f(tbtn,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)end end)
function t:Button(text,icon,callback)
local btn=g("TextButton",{Size=UDim2.new(1,-10,0,42),BackgroundColor3=Color3.fromRGB(20,24,32),Text="",Parent=tcont})
h(btn,10)local bst=i(btn,Color3.fromRGB(50,60,80))
g("ImageLabel",{Size=UDim2.new(0,18,0,18),Position=UDim2.new(0,12,0.5,-9),BackgroundTransparency=1,Image=e[icon]or e.Bolt,ImageColor3=Color3.fromRGB(100,150,255),Parent=btn})
g("TextLabel",{Size=UDim2.new(1,-50,1,0),Position=UDim2.new(0,38,0,0),BackgroundTransparency=1,Text=text or"Button",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=btn})
btn.MouseButton1Click:Connect(function()if callback then callback()end f(btn,{BackgroundColor3=Color3.fromRGB(80,120,200)},.1)task.delay(.1,function()f(btn,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)end)end)
btn.MouseEnter:Connect(function()f(btn,{BackgroundColor3=Color3.fromRGB(30,36,48)},.2)f(bst,{Color=Color3.fromRGB(80,120,200)},.2)end)
btn.MouseLeave:Connect(function()f(btn,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)f(bst,{Color=Color3.fromRGB(50,60,80)},.2)end)
return btn
end
function t:Toggle(text,icon,default,callback)
local on=default or false
local tg=g("Frame",{Size=UDim2.new(1,-10,0,42),BackgroundColor3=Color3.fromRGB(20,24,32),Parent=tcont})
h(tg,10)local tgst=i(tg,Color3.fromRGB(50,60,80))
g("ImageLabel",{Size=UDim2.new(0,18,0,18),Position=UDim2.new(0,12,0.5,-9),BackgroundTransparency=1,Image=e[icon]or e.Check,ImageColor3=Color3.fromRGB(100,150,255),Parent=tg})
g("TextLabel",{Size=UDim2.new(1,-100,1,0),Position=UDim2.new(0,38,0,0),BackgroundTransparency=1,Text=text or"Toggle",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=tg})
local sw=g("Frame",{Size=UDim2.new(0,50,0,26),Position=UDim2.new(1,-62,0.5,-13),BackgroundColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(35,40,50),Parent=tg})
h(sw,13)
local kn=g("Frame",{Size=UDim2.new(0,22,0,22),Position=on and UDim2.new(1,-24,0.5,-11)or UDim2.new(0,2,0.5,-11),BackgroundColor3=Color3.fromRGB(255,255,255),Parent=sw})
h(kn,11)
local btn=g("TextButton",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,Text="",Parent=tg})
btn.MouseButton1Click:Connect(function()
on=not on
f(sw,{BackgroundColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(35,40,50)},.2)
f(kn,{Position=on and UDim2.new(1,-24,0.5,-11)or UDim2.new(0,2,0.5,-11)},.2)
f(tgst,{Color=on and Color3.fromRGB(100,150,255)or Color3.fromRGB(50,60,80)},.2)
if callback then callback(on)end
end)
btn.MouseEnter:Connect(function()f(tg,{BackgroundColor3=Color3.fromRGB(25,30,40)},.2)end)
btn.MouseLeave:Connect(function()f(tg,{BackgroundColor3=Color3.fromRGB(20,24,32)},.2)end)
return{Set=function(v)on=v f(sw,{BackgroundColor3=on and Color3.fromRGB(80,120,200)or Color3.fromRGB(35,40,50)},.2)f(kn,{Position=on and UDim2.new(1,-24,0.5,-11)or UDim2.new(0,2,0.5,-11)},.2)end}
end
function t:Slider(text,icon,min,max,default,callback)
local val=default or min
local sl=g("Frame",{Size=UDim2.new(1,-10,0,60),BackgroundColor3=Color3.fromRGB(20,24,32),Parent=tcont})
h(sl,10)i(sl,Color3.fromRGB(50,60,80))
g("ImageLabel",{Size=UDim2.new(0,18,0,18),Position=UDim2.new(0,12,0,10),BackgroundTransparency=1,Image=e[icon]or e.Settings,ImageColor3=Color3.fromRGB(100,150,255),Parent=sl})
g("TextLabel",{Size=UDim2.new(1,-100,0,20),Position=UDim2.new(0,38,0,10),BackgroundTransparency=1,Text=text or"Slider",TextColor3=Color3.fromRGB(220,230,255),TextSize=13,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=sl})
local vlbl=g("TextLabel",{Size=UDim2.new(0,50,0,20),Position=UDim2.new(1,-62,0,10),BackgroundTransparency=1,Text=tostring(val),TextColor3=Color3.fromRGB(100,150,255),TextSize=12,Font=Enum.Font.GothamBold,Parent=sl})
local bar=g("Frame",{Size=UDim2.new(1,-24,0,8),Position=UDim2.new(0,12,1,-18),BackgroundColor3=Color3.fromRGB(35,40,50),Parent=sl})
h(bar,4)
local fill=g("Frame",{Size=UDim2.new((val-min)/(max-min),0,1,0),BackgroundColor3=Color3.fromRGB(100,150,255),Parent=bar})
h(fill,4)
local fg=Instance.new("UIGradient",fill)fg.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(100,150,255)),ColorSequenceKeypoint.new(1,Color3.fromRGB(150,100,255))}
local sbtn=g("TextButton",{Size=UDim2.new(1,0,1,0),BackgroundTransparency=1,Text="",Parent=bar})
local dragging=false
sbtn.MouseButton1Down:Connect(function()dragging=true end)
b.InputEnded:Connect(function(inp)if inp.UserInputType==Enum.UserInputType.MouseButton1 then dragging=false end end)
b.InputChanged:Connect(function(inp)
if dragging and inp.UserInputType==Enum.UserInputType.MouseMovement then
local mx=inp.Position.X
local bx=bar.AbsolutePosition.X
local bw=bar.AbsoluteSize.X
local pct=math.clamp((mx-bx)/bw,0,1)
val=math.floor(min+(max-min)*pct)
vlbl.Text=tostring(val)
f(fill,{Size=UDim2.new(pct,0,1,0)},.05)
if callback then callback(val)end
end
end)
return{Set=function(v)val=v vlbl.Text=tostring(v)f(fill,{Size=UDim2.new((v-min)/(max-min),0,1,0)},.2)end,Get=function()return val end}
end
function t:Label(text)
local lb=g("TextLabel",{Size=UDim2.new(1,-10,0,30),BackgroundTransparency=1,Text=text or"Label",TextColor3=Color3.fromRGB(120,140,180),TextSize=12,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=tcont})
return lb
end
return t
end
return w
end
return j

local tic=g("ImageLabel",{Size=UDim2.new(0,18,0,18),Position=UDim2.new(0,12,0.5,-9),BackgroundTransparency=1,Image=e[icon]or e.Home,ImageColor3=Color3.fromRGB(140,160,200),Parent=tbtn})
local tname=g("TextLabel",{Size=UDim2.new(1,-44,1,0),Position=UDim2.new(0,36,0,0),BackgroundTransparency=1,Text=name or"Tab",TextColor3=Color3.fromRGB(180,190,220),TextSize=12,Font=Enum.Font.GothamBold,TextXAlignment=Enum.TextXAlignment.Left,Parent=tbtn})

local a,b,c,d,e,f,g,h,i,j,k=game:GetService("TweenService"),game:GetService("UserInputService"),game:GetService("CoreGui"),Instance.new,UDim2.new,UDim.new,Color3.fromRGB,Vector2.new,Enum.Font.Gotham,Enum.EasingStyle.Quart,TweenInfo.new
local function l(m,n,o,p)a:Create(m,k(o or.3,p or j),n):Play()end
local function q(r,s)local t=d(r)for u,v in pairs(s)do if u~="Parent"then t[u]=v end end if s.Parent then t.Parent=s.Parent end return t end
local function w(x,y)return q("UICorner",{CornerRadius=f(0,y or 8),Parent=x})end
local function z(x,A,B)return q("UIStroke",{Color=A or g(80,120,200),Thickness=B or 1,Transparency=.5,Parent=x})end
local C,D={},{}
function C:Notify(E,F,G,H)
local I=c:FindFirstChild("TekkitNotify")or q("ScreenGui",{Name="TekkitNotify",ResetOnSpawn=false,Parent=c})
local J=q("Frame",{Size=e(0,300,0,70),Position=e(1,320,1,-100),BackgroundColor3=g(15,18,25),Parent=I})
w(J,10)z(J,g(100,150,255),1.5)
q("TextLabel",{Size=e(1,-20,0,20),Position=e(0,10,0,10),BackgroundTransparency=1,Text=E or"Notification",TextColor3=g(240,245,255),TextSize=14,Font=i,TextXAlignment=Enum.TextXAlignment.Left,Parent=J})
q("TextLabel",{Size=e(1,-20,0,16),Position=e(0,10,0,32),BackgroundTransparency=1,Text=F or"",TextColor3=g(140,150,180),TextSize=12,Font=i,TextXAlignment=Enum.TextXAlignment.Left,TextTruncate=Enum.TextTruncate.AtEnd,Parent=J})
local K=q("Frame",{Size=e(1,-20,0,3),Position=e(0,10,1,-10),BackgroundColor3=g(35,40,50),Parent=J})
w(K,2)
local L=q("Frame",{Size=e(1,0,1,0),BackgroundColor3=g(100,150,255),Parent=K})
w(L,2)
table.insert(D,J)
local M=0
for N,O in ipairs(D)do if O and O.Parent then M=M+1 l(O,{Position=e(1,-320,1,-20-(M*80))},.3,Enum.EasingStyle.Back)end end
l(J,{Position=e(1,-320,1,-20-(M*80))},.5,Enum.EasingStyle.Back)
task.spawn(function()
local P=G or 3
for Q=100,0,-1 do L.Size=e(Q/100,0,1,0)task.wait(P/100)end
l(J,{Position=e(1,320,J.Position.Y.Scale,J.Position.Y.Offset),BackgroundTransparency=1},.4,Enum.EasingStyle.Back)
task.wait(.4)
for N,O in ipairs(D)do if O==J then table.remove(D,N)break end end
J:Destroy()
M=0
for N,O in ipairs(D)do if O and O.Parent then M=M+1 l(O,{Position=e(1,-320,1,-20-(M*80))},.3,Enum.EasingStyle.Back)end end
end)
return J
end

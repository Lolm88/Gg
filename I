local a,b,c,d,e,f,g,h,i,j,k=game:GetService("TweenService"),game:GetService("UserInputService"),game:GetService("CoreGui"),Instance.new,UDim2.new,UDim.new,Color3.fromRGB,Vector2.new,Enum.Font.Gotham,Enum.EasingStyle.Quart,TweenInfo.new
local function l(m,n,o,p)a:Create(m,k(o or.3,p or j),n):Play()end
local function q(r,s)local t=d(r)for u,v in pairs(s)do if u~="Parent"then t[u]=v end end if s.Parent then t.Parent=s.Parent end return t end
local function w(x,y)return q("UICorner",{CornerRadius=f(0,y or 8),Parent=x})end
local C,D={},{}
function C:Notify(E,F,G)
local H=c:FindFirstChild("TekkitNotify")or q("ScreenGui",{Name="TekkitNotify",ResetOnSpawn=false,Parent=c})
local I=q("Frame",{Size=e(0,300,0,70),Position=e(1,320,1,-100),BackgroundColor3=g(20,20,20),Parent=H})
w(I,8)
q("TextLabel",{Size=e(1,-20,0,20),Position=e(0,10,0,10),BackgroundTransparency=1,Text=E or"Notification",TextColor3=g(255,255,255),TextSize=14,Font=i,TextXAlignment=Enum.TextXAlignment.Left,Parent=I})
q("TextLabel",{Size=e(1,-20,0,16),Position=e(0,10,0,32),BackgroundTransparency=1,Text=F or"",TextColor3=g(180,180,180),TextSize=12,Font=i,TextXAlignment=Enum.TextXAlignment.Left,Parent=I})
local J=q("Frame",{Size=e(1,-20,0,3),Position=e(0,10,1,-10),BackgroundColor3=g(50,50,50),Parent=I})
w(J,2)
local K=q("Frame",{Size=e(1,0,1,0),BackgroundColor3=g(100,150,255),Parent=J})
w(K,2)
table.insert(D,I)
local L=0
for M,N in ipairs(D)do if N and N.Parent then L=L+1 l(N,{Position=e(1,-320,1,-20-(L*80))},.3,Enum.EasingStyle.Back)end end
l(I,{Position=e(1,-320,1,-20-(L*80))},.5,Enum.EasingStyle.Back)
task.spawn(function()
local O=G or 3
for P=100,0,-1 do K.Size=e(P/100,0,1,0)task.wait(O/100)end
l(I,{Position=e(1,320,I.Position.Y.Scale,I.Position.Y.Offset)},.4,Enum.EasingStyle.Back)
task.wait(.4)
for M,N in ipairs(D)do if N==I then table.remove(D,M)break end end
I:Destroy()
L=0
for M,N in ipairs(D)do if N and N.Parent then L=L+1 l(N,{Position=e(1,-320,1,-20-(L*80))},.3,Enum.EasingStyle.Back)end end
end)
end

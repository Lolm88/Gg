bd.MouseEnter:Connect(function()l(bd,{BackgroundColor3=g(35,35,35)},.2)end)
bd.MouseLeave:Connect(function()l(bd,{BackgroundColor3=g(25,25,25)},.2)end)
return{Set=function(v)ba=v bd.Text=v.Name end,Get=function()return ba end}
end
function an:ColorPicker(ao,ap,aq)
local bf=ap or g(100,150,255)
local aN=false
local bg=q("Frame",{Size=e(1,0,0,36),BackgroundColor3=g(18,18,18),ClipsDescendants=true,Parent=ak})
w(bg,6)
q("TextLabel",{Size=e(1,-100,0,36),Position=e(0,10,0,0),BackgroundTransparency=1,Text=ao or"Color",TextColor3=g(200,200,200),TextSize=12,Font=i,TextXAlignment=Enum.TextXAlignment.Left,Parent=bg})
local bh=q("Frame",{Size=e(0,40,0,20),Position=e(1,-48,0,8),BackgroundColor3=bf,Parent=bg})
w(bh,5)
local bi={g(255,0,0),g(255,127,0),g(255,255,0),g(0,255,0),g(0,255,255),g(0,127,255),g(0,0,255),g(127,0,255),g(255,0,255),g(255,255,255),g(180,180,180),g(100,100,100),g(50,50,50),g(0,0,0)}
local bj=q("Frame",{Size=e(1,-16,0,50),Position=e(0,8,0,40),BackgroundTransparency=1,Parent=bg})
q("UIGridLayout",{CellSize=e(0,28,0,20),CellPadding=e(0,4,0,4),Parent=bj})
for aR,bk in ipairs(bi)do local bl=q("TextButton",{Size=e(0,28,0,20),BackgroundColor3=bk,Text="",Parent=bj})w(bl,4)bl.MouseButton1Click:Connect(function()bf=bk bh.BackgroundColor3=bk if aq then aq(bk)end end)bl.MouseEnter:Connect(function()l(bl,{Size=e(0,30,0,22)},.1,Enum.EasingStyle.Back)end)bl.MouseLeave:Connect(function()l(bl,{Size=e(0,28,0,20)},.1)end)end
local aU=q("TextButton",{Size=e(1,0,0,36),BackgroundTransparency=1,Text="",Parent=bg})
aU.MouseButton1Click:Connect(function()aN=not aN l(bg,{Size=e(1,0,0,aN and 100 or 36)},.3,Enum.EasingStyle.Back)end)
aU.MouseEnter:Connect(function()if not aN then l(bg,{BackgroundColor3=g(22,22,22)},.2)end end)
aU.MouseLeave:Connect(function()if not aN then l(bg,{BackgroundColor3=g(18,18,18)},.2)end end)
return{Set=function(v)bf=v bh.BackgroundColor3=v end,Get=function()return bf end}
end
return an
end
local bm=q("UIGridLayout",{CellSize=e(.48,0,0,100),CellPadding=e(.04,0,0,10),FillDirection=Enum.FillDirection.Horizontal,Parent=ag})
bm:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
local bn=0
for aR,bo in pairs(ag:GetChildren())do
if bo:IsA("Frame")and bo.Visible then
bn=math.max(bn,bo.AbsoluteSize.Y)
end
end
end)
return ae
end
a5()
C:Notify("Tekkit Hub","Loaded Successfully!",3)
return R
end
return C

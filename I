aK.InputBegan:Connect(function(aa)if aa.UserInputType==Enum.UserInputType.MouseButton1 then aF=true aG(aa)l(aD,{Size=e(0,14,0,14)},.1)end end)
b.InputEnded:Connect(function(aa)if aa.UserInputType==Enum.UserInputType.MouseButton1 then aF=false l(aD,{Size=e(0,12,0,12)},.1)end end)
b.InputChanged:Connect(function(aa)if aF and aa.UserInputType==Enum.UserInputType.MouseMovement then aG(aa)end end)

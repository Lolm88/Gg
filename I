bc.Focused:Connect(function()l(bb,{Color=g(100,150,255),Transparency=.2,Thickness=1.5},.3)l(ba,{BackgroundColor3=g(20,25,35)},.3)end)
bc.FocusLost:Connect(function(bd)l(bb,{Color=g(60,80,120),Transparency=.5,Thickness=1},.3)l(ba,{BackgroundColor3=g(15,15,22)},.3)if bd and ap then ap(bc.Text)end end)
return{Set=function(v)bc.Text=v end,Get=function()return bc.Text end}
end

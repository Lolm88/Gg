local function aG(aa)local aH=aB.AbsolutePosition.X local aI=aB.AbsoluteSize.X local aJ=math.clamp((aa.Position.X-aH)/aI,0,1)az=math.floor(ax+(ay-ax)*aJ)aE.Text=tostring(az)l(aC,{Size=e(aJ,0,1,0)},.05)l(aD,{Position=e(aJ,0,.5,0)},.05)if aq then aq(az)end end
